SELECT ISSUESLISTS.NAME AS LIST, I.SUMMARY AS TASK_NAME
FROM ISSUES I JOIN ISSUESLISTS on I.ISSUESLIST_ID = ISSUESLISTS.ID;

SELECT I.SUMMARY AS TASK_NAME, U.FIRSTNAME, U.LASTNAME
FROM USERS U JOIN ISSUES I ON U.ID = I.USER_ID_ASSIGNEDTO;

SELECT U.FIRSTNAME, U.LASTNAME, COUNT(*) AS POSTS_NUMBER
FROM USERS U JOIN ISSUES I on U.ID = I.USER_ID_ASSIGNEDTO
GROUP BY I.USER_ID_ASSIGNEDTO
HAVING COUNT(*) > 0
ORDER BY COUNT(*);